{
  "name": "atom-shell-commands",
  "version": "1.1.3",
  "description": "Customize shell commands for atom. Similar to 'Run Commands' in NotePad++, 'Shell Commands' in TextMate, 'User Tool' in EditPlus/UltraEdit and 'External Tool' in GEdit",
  "main": "./lib/atom-shell-commands",
  "keywords": [
    "build",
    "command",
    "user",
    "run",
    "shell"
  ],
  "homepage": "https://github.com/skywind3000/atom-shell-commands",
  "dependencies": {
    "win-spawn": "^2.0.0",
    "space-pen": "^4",
    "xregexp": "^2.0.0"
  },
  "author": {
    "name": "skywind3000"
  },
  "repository": "https://github.com/skywind3000/atom-shell-commands",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "bugs": {
    "url": "https://github.com/skywind3000/atom-shell-commands/issues"
  },
  "_atomModuleCache": {
    "version": 1,
    "dependencies": [
      {
        "name": "win-spawn",
        "version": "2.0.0",
        "path": "node_modules/win-spawn/index.js"
      }
    ],
    "extensions": {
      ".js": [
        "lib/atom-shell-commands.js",
        "lib/config.js",
        "node_modules/win-spawn/index.js"
      ],
      ".json": [
        "node_modules/win-spawn/package.json",
        "package.json"
      ]
    },
    "folders": [
      {
        "paths": [
          "lib",
          "menus",
          ""
        ],
        "dependencies": {
          "win-spawn": "^2.0.0"
        }
      }
    ]
  }
}
